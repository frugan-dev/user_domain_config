{
    "name": "frugan-dev/user_domain_config",
    "description": "Per-User-Domain Configuration Plugin for Roundcube",
    "license": "GPL-3.0-or-later",
    "type": "roundcube-plugin",
    "keywords": [
        "roundcube",
        "roundcube-plugin"
    ],
    "authors": [
        {
            "name": "Frugan",
            "email": "dev@frugan.it",
            "homepage": "https://frugan.it",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/frugan-dev/user_domain_config",
    "require": {
        "roundcube/plugin-installer": ">=0.3.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.42",
        "friendsofphp/php-cs-fixer": "^3.57",
        "nikic/php-parser": "^4.19",
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "phpro/grumphp-shim": "^1.13",
        "phpstan/phpstan": "^1.11",
        "povils/phpmnd": "^3.4",
        "rector/rector": "^1.1",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^5.24"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://plugins.roundcube.net"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpro/grumphp-shim": true,
            "roundcube/plugin-installer": true
        },
        "platform": {
            "php": "7.4"
        },
        "platform-check": true,
        "sort-packages": true
    },
    "extra": {
        "roundcube": {
            "min-version": "1.0.0"
        }
    }
}
